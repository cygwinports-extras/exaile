inherit python

DESCRIPTION="GTK+ music player, similar to AmaroK"
HOMEPAGE="http://www.exaile.org/"
SRC_URI="http://launchpad.net/exaile/${PV[1]}.${PV[2]}.${PV[3]}/${PV}/+download/${P}.tar.gz"

src_compile() { :; }

src_install() {
	cd ${S}
	cyginstall PREFIX=/usr

	python_optimize /usr/lib/${PN} /usr/share/${PN}
}
